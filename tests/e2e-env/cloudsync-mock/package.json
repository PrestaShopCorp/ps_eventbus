{
  "version": "1.0.0",
  "name": "cloudsync-mock",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "build": "npx swc ./src -d dist",
    "start:dev": "npx swc ./src -w -d dist",
    "start": "node dist/app.js",
    "lint": "npx eslint '{src,e2e}/**/*.{ts,js}'",
    "lint:fix": "npx eslint '{src,e2e}/**/*.{ts,js}' --fix",
    "test": "npx jest",
    "docker:build": "docker compose -f ../docker-compose.yml build cloudsync-mock-test",
    "docker:up": "docker compose -f ../docker-compose.yml up -d --wait",
    "docker:down": "docker compose -f ../docker-compose.yml down"
  },
  "dependencies": {
    "@jest/globals": "^29.4.3",
    "@types/express": "^4.17.17",
    "express-multipart-file-parser": "^0.1.2",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.26",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "chokidar": "^3.5.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-deprecation": "^1.3.3",
