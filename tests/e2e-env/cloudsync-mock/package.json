{
  "version": "1.0.0",
  "name": "cloudsync-mock",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "build": "npx swc ./src -d dist",
    "build:watch": "npx swc ./src -w -d dist",
    "start": "pnpm build && pnpm start:prod",
    "start:prod": "node dist/app.js",
    "lint": "npx eslint '{src,e2e}/**/*.{ts,js}'",
    "lint:fix": "npx eslint '{src,e2e}/**/*.{ts,js}' --fix",
    "test": "npx jest",
    "docker:build": "docker compose -f ../docker-compose.yml build ps-eventbus-tests",
    "docker:up": "docker compose -f ../docker-compose.yml up -d --wait",
    "docker:down": "docker compose -f ../docker-compose.yml down"
  },
  "dependencies": {
    "@jest/globals": "^29.5.0",
    "express": "^4.18.2",
    "express-multipart-file-parser": "^0.1.2",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.66",
    "@swc/jest": "^0.2.26",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "chokidar": "^3.5.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-vue": "^9.15.0",
    "jest": "^29.5.0",
    "jest-expect-message": "^1.1.3",
    "jest-extended": "^4.0.0",
    "jest-mock-extended": "^3.0.4",
    "mockttp": "^3.7.5",
    "npm-check-updates": "^16.10.12",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5",
    "yaml-eslint-parser": "^1.2.2"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  }
}
