{
  "name": "cloudsync-mock",
  "version": "0.0.0",
  "main": "dist/app.js",
  "private": true,
  "scripts": {
    "start": "node dist/app.js",
    "start:dev": "npm run build:dev & npx chokidar \"dist/**/*.js\" --initial --command \"node dist/app.js\" --silent",
    "build": "npx swc ./src -d dist",
    "build:dev": "swc ./src -d ./dist --watch --log-watch-compilation",
    "clean": "rimraf ./dist"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.100",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "chokidar": "^3.5.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "engines": {
    "yarn": "please use pnpm",
    "npm": "please use pnpm",
    "node": ">=20",
    "pnpm": ">=8"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
